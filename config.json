{
    "config":{
        "title":"",
        "languages": ["ES","PT","EN"],
        "autoSort": true,
        "confimOnShort":true,
        "hoverProb":true,
        "outputReplaceRegExp": "(([a-z]\\. +(?=\\d))|([Ii]n:))"
    },
    
    "splitters":[
        {"name": ".","regExp": "^\\.$","replace": "(([A-Za-zÀ-ÿ]{2,}|\\d|[A-Z]+[a-z]*|et al|\\))\\.)(?!( *([,;]|\\d+[^\\d]\\d*[^\\.]|[A-Z]+[\\.\/] *| *ed?\\.))|([a-z]{3}[^\\w]|\\d|[a-z]+))"},
        {"name": ",","regExp": "^,$","replace":"(([0-9]+| [A-Z]*[a-zà-ÿ]+|[a-z][^A-Z\\.]+),)(?! *([A-Z]\\.|[A-Z]*[a-zà-ÿ]{4,}]))"},
        {"name": ":","regExp": "^:$","replace": "(([^Iin]):)(?! *(\\d+|\\/|([A-Z]?[a-zà-ÿ]+( +[a-z\\-à-ÿ]+){2,})))"},
        {"name": ";","regExp": "^;$","replace": "([^.] *;)(?! +[A-Z]{2,})"},
        {"name": "(","regExp": "^\\($","replace": "(\\()"},
        {"name": ")","regExp": "^\\)$","replace": "(\\))"}
    ],
   
    "fields":[
        {"name":"authors",
            "type":"word",
            "size":1,
            "regExp":["([A-Z]{1}[a-z]+, +[A-Z]+)","(\\S+[,;] )+", "([A-Z]{3,})","([A-Z]{1}\\. ?)+","([A-Z]{3},)","([A-Z]{1}\\.;? +[A-Z])","(et al)","(_){4,6}","((Ed)|(Org)|(Coo?rd)|(Comp))s?"],
            "splitReplaceAfter":[ "([\\&;])", "([A-Z]+\\.? *,)"]
        },
        {"name":"documentTitle",
            "type":"word",
            "size":1,
            "regExp":["(([A-Z]*[\\wà-ÿ-]{1,} +){2,})"]
        },
        {"name":"year",
            "type":"word",
            "size"   :0,  
            "regExp":["^([0-9]{4})$", "(date unknown)" ,"((no prelo)|(in press)|(en prensa))"]
        },
        {"name":"documentSetTitle",
            "type":"word",
            "size":1,
            "regExp":["(([A-Za-zÀ-ÿ]+ *){2,})", "([Ii]n: +)", "( [A-Za-zÀ-ÿ]{4,})+:( [A-Za-zÀ-ÿ]+)+","[A-Z]{1}[a-zà-ÿ]{2,} ?.* [A-Z]{1}[a-zà-ÿ]{2,}]"]
        },
        {"name":"edition",
            "type":"word",
            "size":0,
            "regExp":["[0-9]* *(a|(nd)|(rd)|(th)|e)\\.","([Ee]d\\.)"]
        },
        {"name":"publisher",
            "type":"word",
            "size":1,
            "regExp":["([A-Z]+([a-z]+|\\.) +)"]
        },
        {"name":"pages",
            "type":"word",
            "size"   :0,
            "regExp":["(pp?\\. +)?\\d+-\\d+", "p(((a|á)g)|p)?s?\\."]
        },
        {"name":"volume",
            "type":"word",
            "size"   :0,
            "regExp":["[Vv](ol)?\\. +\\d", "^[0-9]{1,3}$"]
        },
        {"name":"location",
            "type":"word",
            "size"   :0,
            "regExp":["[Ss]\\.[Ll]\\.","([A-Z]{1}[a-zà-ÿ]{2,} *){1,4}", "^[A-Z]{2,3}$"]
        },
        {"name":"course",
            "type":"word",
            "size"   :1,  
            "regExp":["([A-Z]+[a-z]+ ){2,}"]
        },
        {"name":"number",
            "type":"word",
            "size"   :0,
            "regExp":["[Nn]o?\\. +\\d", "^[0-9]{1,3}$"]
        },
        {"name":"publishingCompany",
            "type":"word",
            "size"   :1,
            "regExp":["([A-Z]{1}[A-Za-zÀ-ÿ]+ +[a-z]+ +[A-Z]{1}[A-Za-zÀ-ÿ]+)","s\\.n\\."]
        },
        {"name":"institution",
            "type":"word",
            "size"   :1,
            "regExp":["([A-Z]+[a-z]+ ){1,}"]
        },
        {"name":"url",
            "type":"word",
            "size"   :1,
            "regExp":["((w{3})|(url)|(com)|(https?:[A-Za-zÀ-ÿ]*))$","[A-Za-zÀ-ÿ]ww\\.[\\\/]"]
        },
        {"name":"doi",
            "type":"word",
            "size"   :1,
            "regExp":["(doi)|(DOI):", "(ISBN)|(isbn)","(ISSN)|(issn)", "\\d{2}-\\d{3}-\\d{4}-\\d{1}"]
        },
        {"name":"month",
            "type":"word",
            "size"   :0,
            "regExp":["^[a-z]{3}$"]
        }
    ],
                        
